function [] = task3a()
    clc;
    close all;
%     image = imread('img2.png');
%     image1 = imread('img1.png');
    image = imread('key3.jpg');
    image1 = imread('key1.jpg');
    figure(1);
    imshow(image);
    [x,y]=ginput(4);
    figure(2);
    imshow(image1);
    [x1,y1]=ginput(4);
    A = zeros(8,9);
    for i=1:4
        A(2*i-1, :) = [x(i),y(i), 1,0,0,0,-x(i)*x1(i), -x1(i)*y(i), -x1(i)];
        A(2*i, :) = [0,0,0,x(i), y(i),1,-x(i)*y1(i), -y1(i)*y(i), -y1(1)];
    end

    [U,S,V] = svd(A);
    h = V(:,9);
%     h1 = reshape(h,3,3);
    h1 = [h(1) h(2) h(3); h(4) h(5) h(6); h(7) h(8) h(9);];
    t = projective2d(h1);
    imout = imwarp(image1, t, 'OutputView',imref2d(size(image)));
    figure(3);
    imshow(imout);
    
    
end


