function [] = superimpose()
    clc;
    close all;
    image1 = imread('key1.jpg');
    image2 = imread('key3.jpg');
%     figure(1)
%     imshow(image2)
    figure(2)
    imshow(image1)
    [x,y]=ginput(4)
    bw = poly2mask(x,y, size(image2,1), size(image2,2))
    wb = imcomplement(bw);
    disp(size(bw))
    image2(bw ~= 0) = 2;
    imshow(image2, [])
end
